# Executor Agent environment (recommended for systemd or manual export)
# Copy to `.env` and set real values. Treat tokens as passwords.

# Required: provisioned executor token from Secrux control plane
EXECUTOR_TOKEN=REPLACE_ME

# Optional: override engine images without editing config.json
# ENGINE_IMAGE_MAP format: "engine=image,engine2=image2"
ENGINE_IMAGE_MAP=
ENGINE_SEMGREP_IMAGE=
ENGINE_TRIVY_IMAGE=

# Optional: proxy passthrough for engines (if needed)
HTTP_PROXY=
HTTPS_PROXY=
NO_PROXY=localhost,127.0.0.1

